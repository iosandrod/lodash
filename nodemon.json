{
  "restartable": "rs",
  "ignore": [".git", ".svn", "node_modules/**/node_modules"],
  "verbose": true,
  "execMap": {
    "js": "node --harmony",
    "mjs": "node --harmony"
  },
  "watch": ["./*.ts", "./*.js", "./*.mjs", "./js/*.js","./*.cs"],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "cs",
  "exec":"dotnet script main.cs"
}
